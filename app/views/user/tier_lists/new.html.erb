<h1>User::TierLists#new</h1>


<div>
  <table>
    <tr>
      <td>テーマ名</td>
      <td><%= @theme.name %></td>
    </tr>
    <tr>
      <td>タグ</td>
      <td>
        <% @theme.tags.each do |tag| %>
          <%=tag.name%>,
        <% end %>
      </td>
    </tr>
  </table>
</div>

<div>
  <%= render 'tier_list', theme: @theme %>
</div>


<p>稼働テスト（稼働してない）</p>
<script type="text/javascript">
  $(document).ready(function() {
    $("p").text("jQuery稼働テスト(稼働中)");
  });
</script>
